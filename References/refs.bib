@Article{Lee2013,
  author       = {Lee, Jinsoo and Han, Wook-Shin and Kasperovics, Romans and Lee, Jeong-Hoon},
  title        = {{An in-depth comparison of subgraph isomorphism algorithms in graph databases}},
  pages        = {133--144},
  issn         = {21508097},
  abstract     = {Finding subgraph isomorphisms is an important problem in many applications$\backslash$nwhich deal with data modeled as graphs. While this problem is NP-hard,$\backslash$nin recent years, many algorithms have been proposed to solve it in$\backslash$na reasonable time for real datasets using different join orders,$\backslash$npruning rules, and auxiliary neighborhood information. However, since$\backslash$nthey have not been empirically compared one another in most research$\backslash$nwork, it is not clear whether the later work outperforms the earlier$\backslash$nwork. Another problem is that reported comparisons were often done$\backslash$nusing the original authors' binaries which were written in different$\backslash$nprogramming environments. In this paper, we address these serious$\backslash$nproblems by re-implementing five state-of-the-art subgraph isomorphism$\backslash$nalgorithms in a common code base and by comparing them using many$\backslash$nreal-world datasets and their query loads. Through our in-depth analysis$\backslash$nof experimental results, we report surprising empirical findings.},
  year         = {2013},
  doi          = {10.14778/2535568.2448946},
  journal = {Proceedings of the 39\textsuperscript{th} international conference on Very Large Data Bases},
  url          = {http://dl.acm.org/citation.cfm?id=2448936.2448946},
}

