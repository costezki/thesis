@ARTICLE{Mossakowski07,
  author = {Till Mossakowski and Andrzej Tarlecki and Razvan Diaconescu},
  title = {{W}hat is a logic translation?},
  year = {2009},
  journal = {Logica Universalis},
  volume = {3},
